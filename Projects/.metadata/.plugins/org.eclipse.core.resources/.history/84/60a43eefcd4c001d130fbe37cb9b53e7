#include "STD_TYPES.h"
#include "BIT_MATH.h"
#include "DIO_interface.h"

void HSS_voidSetNumber(SS ptr_SS)
{
	MDIO_u8SetPortDirection(ptr_SS.Port,PORT_OUTPUT);
	MDIO_u8SetPinDirection(ptr_SS.EnablePort,ptr_SS.EnablePin,PIN_OUTPUT);

}
