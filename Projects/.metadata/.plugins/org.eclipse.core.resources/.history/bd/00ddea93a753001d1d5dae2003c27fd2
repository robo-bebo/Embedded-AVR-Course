#include"STD_TYPES.h"
#include"BIT_MATH.h"

#include"GIE_private.h"
#include"GIE_interface.h"

void MGIE_voidEnable(void)
{
	SET_BIT(SREG,SREG_I);
}

void MGIE_voidDisable(void)
{
	CLR_BIT(SREG,SREG_I);
}
